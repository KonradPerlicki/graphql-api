{
    "name": "react-node-template",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "npm run build && node ./dist/index.js",
        "build": "tsc",
        "dev": "ts-node-dev --poll --respawn --transpile-only src/index.ts",
        "format": "prettier --write src/",
        "lint": "eslint src/",
        "typeorm": "typeorm-ts-node-commonjs",
        "show-migrations": "npm run typeorm -- migration:show -d ./src/data-source.ts",
        "run-migrations": "npm run typeorm -- migration:run -d ./src/data-source.ts",
        "revert-migration": "npm run typeorm -- migration:revert -d ./src/data-source.ts",
        "new-migration": "npm run typeorm migration:create ./src/migrations/migration",
        "test": "jest --watchAll"
    },
    "keywords": [],
    "author": "Konrad Perlicki <konrad.perlicki01@gmail.com>",
    "license": "ISC",
    "dependencies": {
        "@aws-sdk/client-elasticache": "^3.272.0",
        "apollo-server-core": "^3.12.0",
        "apollo-server-express": "^3.12.0",
        "bcryptjs": "^2.4.3",
        "class-validator": "^0.14.0",
        "connect-pg-simple": "^8.0.0",
        "connect-redis": "^6.1.3",
        "cors": "^2.8.5",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "ioredis": "^5.3.0",
        "lodash": "^4.17.21",
        "morgan": "^1.10.0",
        "node-cache": "^5.1.2",
        "nodemailer": "^6.9.1",
        "pg": "^8.9.0",
        "reflect-metadata": "^0.1.13",
        "type-graphql": "^1.1.1",
        "typeorm": "^0.3.12",
        "uuid": "^9.0.0",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@faker-js/faker": "^7.6.0",
        "@types/bcryptjs": "^2.4.2",
        "@types/connect-pg-simple": "^7.0.0",
        "@types/connect-redis": "^0.0.19",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.17.6",
        "@types/jest": "^29.5.0",
        "@types/lodash": "^4.14.191",
        "@types/morgan": "^1.9.4",
        "@types/node": "^18.13.0",
        "@types/node-cache": "^4.2.5",
        "@types/nodemailer": "^6.4.7",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.51.0",
        "@typescript-eslint/parser": "^5.51.0",
        "eslint": "^8.34.0",
        "jest": "^29.5.0",
        "prettier": "^2.8.4",
        "ts-jest": "^29.0.5",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^4.9.5"
    }
}
